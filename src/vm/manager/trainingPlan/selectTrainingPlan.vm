<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>



#parse("snippets/learnerNewHead.vm")
<script language="javascript" type="text/javascript" src="javascript/360js.js"></script>
<script type="text/javascript" language="javascript">
function FormSubmit(buttonclick) {
    //alert(buttonclick);
    document.getElementById('action').value=buttonclick;
    document.enrollmentForm.submit();
}
$(document).ready(function(){
        
    });
</script>

<script src="javascript/360js.js" language="javascript" type="text/javascript" ></script>

</head>

<body>
<form name="trainingPlan" action="mgr_assignTraningPlan.do" method="POST">

<table id="table-layout" border="0" cellpadding="0" cellspacing="0"> <!-- Table 1 -->
	<tr> <!-- First Block -->
		<td id="header" border="0" cellpadding="0" cellspacing="0" colspan="2">
        <!-- header -->
        #parse("snippets/managerWizardHeader.vm")
        <!-- header-end -->
		</td>
	</tr>
	
 <tr id="main"> <!-- SECOND BLOCK -->
	<td id="left-navigation" valign="top" style="width:195px;">
		<table id="table-bands" border="0" cellpadding="0" cellspacing="0"> <!-- Table 3 -->
                <!-- left-pannel-content -->
                <div>
                        <div id="l-menu-back">
                                <div class="$brander.getBrandElement("lms.assignEnrollment.cssClass.l-menu-back-text-h")">$brander.getBrandElement("lms.manager.assignTrainingPlan.caption.lmenuHeader") <br>$brander.getBrandElement("lms.manager.assignTrainingPlan.caption.lmenu.step1-6")</div>
                                <div class="$brander.getBrandElement("lms.manageUser.editLearner.cssClass.l-menu-back-text-act")">$brander.getBrandElement("lms.manager.assignTrainingPlan.caption.lmenu.step1")</div>
                                <div class="$brander.getBrandElement("lms.manageUser.editLearner.cssClass.l-menu-back-text")">$brander.getBrandElement("lms.manager.addTrainingPlans.leftMenuSelectTrainingPlan")</div>
                                <div class="$brander.getBrandElement("lms.manageUser.editLearner.cssClass.l-menu-back-text")">$brander.getBrandElement("lms.manager.addTrainingPlans.leftMenuSelectLearnersor")</div>
                                <div class="$brander.getBrandElement("lms.manageUser.editLearner.cssClass.l-menu-back-text")">$brander.getBrandElement("lms.manager.assignTrainingPlan.caption.lmenu.step4")</div>
                                <div class="$brander.getBrandElement("lms.manageUser.editLearner.cssClass.l-menu-back-text")">$brander.getBrandElement("lms.manager.assignTrainingPlan.caption.lmenu.step5")</div>
                                <div class="$brander.getBrandElement("lms.manageUser.editLearner.cssClass.l-menu-back-text")">$brander.getBrandElement("lms.assignEnrollment.caption.wizardStep6b")</div>
                        </div>
                </div>
                <!-- left-pannel-content-end -->
				</table>
			</td>
			
                <!-- right-pannel-content -->
               <TD id="content" >
			   	   <div id="scrollable">
                        #springBind("trainingPlanForm.trainingPlanName")
                        #if($status.error)
                        <!-- error-box -->
                        <div id="message_box">
                                <img id="close_message" style="float:right;cursor:pointer"  src="$brander.getBrandElement("lms.errorMessage.pixel.12-em-cross.png")" />
                                #foreach($errorCode in $status.errorCodes)
                                #set ($error = $brander.getBrandElement($errorCode))
                                <div class="width40pct">
                                <div class="message_box_align"><img src="$brander.getBrandElement("lms.errorMessage.pixel.addLearner_06.gif")" width="14" height="14" alt="" /></div>
                                <div class="message_box_align">$error</div>
                                </div>
                                #end
                        </div>
                        <!-- error-box -->
                        #end

                        <!-- Logo+Manage-User -->
		<table id="table-page-description" border="0" cellpadding="0" cellspacing="0" style="margin-top:15px;margin-bottom:15px">
		    <tr>
			<td id="page-icon" class="page-icon-plan">
			    &nbsp;
			</td>
			<td id="page-description">
			    <div id="page-heading">$brander.getBrandElement("lms.manager.assignTrainingPlan.caption.title")</div>
			    <div id="page-help">$brander.getBrandElement("lms.manager.assignTrainingPlan.caption.desc1")</div>
			</td>
		    </tr>
		</table>                         
                        <!-- Logo+Manage-User-end -->

                        <!-- Permissions -->
                        <div class="$brander.getBrandElement("lms.sendEmail.cssClass.content-Block")">
                                <div id="e12" class="$brander.getBrandElement("lms.sendEmail.cssClass.Block-Contain-header")">
                                        <div id="e13" class="$brander.getBrandElement("lms.sendEmail.cssClass.Search-Containerheaderleft")">$brander.getBrandElement("lms.manager.assignTrainingPlan.caption.select-Training-Plan")</div>
                                </div>
                                <!-- User-left-right-list -->
                                <div id="e7b" class="$brander.getBrandElement("lms.sendEmail.cssClass.Search-inputfield-box3")">
                                        <!-- s -->
                                        <div id="e12" class="$brander.getBrandElement("lms.sendEmail.cssClass.search-content-row1a")"><!-- using for space --></div>
                                        <!-- 1 -->
                                        <div id="e12" class="$brander.getBrandElement("lms.sendEmail.cssClass.search-content-row")">
                                                <div class="Org-grp_4c">$brander.getBrandElement("lms.manager.assignTrainingPlan.caption.assign-Enrollments-To")</div>
                                        </div>
                                         #set($count=0)
                                        #foreach($trPlan in $trainingPlanNames)
                                                #springBind("trainingPlanForm.selectedTrainingPlanId")
                                                <div id="e13" class="$brander.getBrandElement("lms.manageUser.addNewLearner.cssClass.search-content-row")">
                                                        <div class="$brander.getBrandElement("lms.assignEnrollment.cssClass.head_1a-c")"></div>
                                                        <div class="$brander.getBrandElement("lms.assignEnrollment.cssClass.btn_radio1c")"><input type="radio" id="Learner" name="selectedTrainingPlanId"  #if($count== 0) checked #end value="$trPlan.id" /></div>
                                                         #set($count=1)
                                                        <div class="$brander.getBrandElement("lms.addSecurityRole.Org-grp_4c")">$trPlan.name</div>
                                                </div>
                                        #end
                                        <!-- s -->
                                        <div id="e12" class="$brander.getBrandElement("lms.sendEmail.cssClass.search-content-row1a")"><!-- using for space --></div>
                                        <!--  -->
                                </div>
                                <!-- End-User-left-right-list -->
                        </div>
                        <!-- End-Permissions -->

                        <!-- space -->
                        <div class="$brander.getBrandElement("lms.sendEmail.cssClass.manager-bottom-space")"></div>
                        <!-- space -->

                        <!-- button-div -->
                        <div class="$brander.getBrandElement("lms.sendEmail.cssClass.managerBtn")">
                                <input type="hidden" id="currentpage" name="_page" value="0"/>
                                <input type="hidden" id="wizardSubmitter" name="wizardSubmitter" />
                               <div style="float:right; margin-top:3px;">
								  <a onClick="submitButtonForm(document.trainingPlan,'wizardSubmitter','_target1');" style="text-decoration:none;">
									  <div class="btn_start"></div>
									  <div class="btn_normal">Next</div>
									  <div class="btn_end"></div>
								  </a>
								</div>
								<div style="float:left; margin-top:3px;">
									<a onClick="submitButtonForm(document.trainingPlan,'wizardSubmitter','_cancel');" style="text-decoration:none;">
										<div class="btn_start"></div>
										<div class="btn_normal">Cancel</div>
										<div class="btn_end"></div>
									</a>
								</div>	
                        </div>
                        <!-- scrollable-div -->

                        <!-- space -->
                        <div class="$brander.getBrandElement("lms.sendEmail.cssClass.manager-bottom-space")"></div>
                        <!-- space -->

                <!-- right-pannel-content-end -->
                </div>
        <!-- content2-area-end -->
        </td></tr>
		<tr> <!-- Footer BLOCK -->
	<td id="footer" colspan="2">
<!-- footer -->
	#parse("snippets/managerFooter.vm")
<!-- footer-end -->
	 </td></tr>
</table>
</form>
</body>
</html>