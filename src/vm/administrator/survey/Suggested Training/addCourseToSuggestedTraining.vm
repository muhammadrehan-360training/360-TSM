<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head>    #parse("snippets/compliance/complianceHead.vm")    <script type='text/javascript' src='javascript/jquery.simplemodal.js' ></script>    <script type="text/javascript" language="javascript" src="javascript/js_manager.js"></script>    <script src="javascript/360js.js" language="javascript" type="text/javascript" ></script>    <script language="javascript" type="text/javascript">        $(document).ready(function(){           			$('#content2a').height($(window).height() - ($('#header').height()+$('#footer').height()));			if ($('#right-pannel').height()<$('#content2a').height()) {				$('#right-pannel').height($('#content2a').height());			}			$('#sideBar').height($('#right-pannel').height());						$('#content2a').scroll(function(){                $('#message_box').animate({                	top:$('#content2a').scrollTop()+"px" },{queue: false, duration: 350});                });                                $('#close_message').click(function(){	                $("#message_box").animate({	                   opacity: .5	                }, "slow", "linear", function(){$("#message_box").hide();} );            });        });        function changeRowColor(checkbox) {            var tr=checkbox.parentNode.parentNode;            var tdarr=$(tr).find("td");            if (checkbox.checked) {                checkbox.value=true;                for(i=0;i<tdarr.size();i++){                    tdarr[i].style.background="#E9F9FE";                }            } else {                checkbox.value=false;                for(i=0;i<tdarr.size();i++){                    tdarr[i].removeAttribute("style");                }            }        }        function checkAll( countCheckBoxes ) {            var objCheckAll=document.getElementById("chkAll");            if (objCheckAll.checked) {                for(var i = 0; i < countCheckBoxes; i++){                    var objCheckBox=document.getElementById("chk"+i);                    objCheckBox.checked = true;                    changeRowColor(objCheckBox);                }            }else{                for(var i = 0; i < countCheckBoxes; i++){                    var objCheckBox=document.getElementById("chk"+i);                    objCheckBox.checked = false;                    changeRowColor(objCheckBox);                }            }        }        function search() {            document.getElementById('action').value='search';            document.searchCoursesForm.submit();        }        function doFinish() {            document.getElementById('action').value="";        }    </script></head><body><form name="searchCoursesForm" method="post" action="adm_addSuggestedTraining.do">    #springBind("addCoursesInSuggestedTraining.action")    <input type="hidden" id="action" name="action" value=""/> <table id="table-layout" border="0" cellpadding="0" cellspacing="0"> <!-- Table 1 -->	<tr> <!-- First Block -->		<td id="header" border="0" cellpadding="0" cellspacing="0" colspan="2">   <!--Header Start -->    #parse("snippets/adminWizardHeader.vm")    <!-- Header End -->	</td></tr>	    <!-- Body Start -->		<tr id="main"> <!-- SECOND BLOCK -->	<td id="left-navigation" valign="top" style="width:195px;">		<table id="table-bands" border="0" cellpadding="0" cellspacing="0"> <!-- Table 3 -->        <div>            <div id="l-menu-back">                <div class="l-menu-back-text-h">$brander.getBrandElement("lms.administrator.searchMember.caption.addcourseswizardshowing")</div>                <div class="l-menu-back-text-act">$brander.getBrandElement("lms.manager.addTrainingPlans.leftMenuSelectCourses2")</div>                            </div>        </div>		</table></td>		    <TD id="content" >	<div id="scrollable">            #springBind("addCoursesInSuggestedTraining.courses")            #if($status.error)                <div id="message_box">                    <img id="close_message" style="float:right;cursor:pointer"  src="$brander.getBrandElement("lms.errorMessage.pixel.12-em-cross.png")" />                    #foreach($errorCode in $status.errorCodes)                    #set ($error = $brander.getBrandElement($errorCode))                    <div class="width40pct">                        <div class="message_box_align"><img src="$brander.getBrandElement("lms.errorMessage.pixel.addLearner_06.gif")" width="14" height="14" alt="" /></div>                        <div class="message_box_align">$error</div>                    </div>                   #end                </div>            #end           <!-- Logo+Manage-User -->            <div id="pageDescription" class="page_description">                  <div id="pageIcon" class="icon_surveys">&nbsp;</div>                  <div id="pageHeading" class="page_heading">$brander.getBrandElement("lms.surveys.managesurveys.caption.AddCourses")</div>                  <div id="detailsHolder">$brander.getBrandElement("lms.manageSurvey.addSuggestedTrainingCourses.textDescription")</div>            </div>            <!-- Logo+Manage-User-end -->            <div class="search_box_result" >                <div id="resultHeader" class="box_header">                    <div id="searchBox" style="float:left; height:150px; width:290px; left:47%;" class="visible">                        $brander.getBrandElement("lms.administrator.adddistributorentitlement.caption.SearchCourses")<br/><br/><br/>                        #springBind("addCoursesInSuggestedTraining.courseName")                        <div style="float:left">$brander.getBrandElement("lms.manager.addCustomCourses.caption.CourseName")</div> <input type="text" name="$!{status.expression}" value="$!{status.value}" style="font-size:9px; float:right; margin-right:15px; width:170px;"/><br/><br/>                        #springBind("addCoursesInSuggestedTraining.courseId")                        <div style="float:left">$brander.getBrandElement("lms.accraditation.editApproval.caption.CourseID")</div> <input type="text" name="$!{status.expression}" value="$!{status.value}" style="font-size:9px; float:right; margin-right:15px; width:170px;"/><br/><br/>                        #springBind("addCoursesInSuggestedTraining.keywords")                        <div style="float:left">$brander.getBrandElement("lms.instructor.addSynCourse.caption.keywords")</div> <input type="text" name="$!{status.expression}" value="$!{status.value}" style="font-size:9px; float:right; margin-right:15px; width:170px;"/><br/><br/>                        <div id="search-courses-search" style="float:right; margin-top:2px; margin-right:15px;" onclick="search();">                            <div class="btn_start"></div>                            <div class="btn_normal">$brander.getBrandElement("lms.administrator.searchMember.caption.search")</div>                            <div class="btn_end"></div>                        </div>                        <div id="search-courses-cancel" style="float:right; margin-top:2px; " onClick="showSearchBox(this);">                            <div class="btn_start"></div>                            <div class="btn_normal">$brander.getBrandElement("lms.manageUser.addNewLearner.caption.btnCancel")</div>                            <div class="btn_end"></div>                        </div>                    </div>                    #springBind("addCoursesInSuggestedTraining.courses")                    #set($courses = $!{status.value})                    <div class="result_left_side">                        <div id="search-courses" style="float:left; margin-top:2px;" onClick="showSearchBox(this);">                            <div class="btn_start"></div>                            <div class="btn_normal" >$brander.getBrandElement("lms.administrator.searchMember.caption.search")</div>                            <div class="btn_end"></div>                        </div>                        <!-- Showing Results -->                        <div id="resultDescription" class="box_description">$brander.getBrandElement("lms.administrator.searchMember.caption.showing0") $courses.size() of $courses.size()</div>                    </div>                </div>                #if( $courses.size() > 0)                #set($ind = 0)                #set($listSize = $courses.size() - 1)                <div id="searchResult_2">                    <table width="100%" cellpadding="0" cellspacing="0" style="margin-top:1px;">                        <tr bgcolor="#efefef">                            <td class="check_box_top" width="17px"><input type="checkbox" id="chkAll" style="margin-left:0px" onClick="checkAll($courses.size());"/></td>                            <td class="box_subheader" style="border-right:none;"><span style="margin-left:4px;">$brander.getBrandElement("lms.manageUser.searchLearner.caption.Courses")</span></td>                        </tr>                        #foreach($course in $courses)                            #if($ind%2 == 0)                                #set($rowIndex="row_1")                            #else                                #set($rowIndex="row_2")                            #end                            <tr>                                #springBind("addCoursesInSuggestedTraining.courses[$ind].selected")                                <td class="$rowIndex" width="17px" #if($listSize == $ind) style="border-bottom:none;" #end><input type="checkbox" name="$!{status.expression}" value="$!{status.value}" id="chk$ind" onclick="changeRowColor(this);" style="margin-left:-2px"/></td>                                <td class="$rowIndex" #if($listSize == $ind) style="border-bottom:none;" #end>$!course.course.courseTitle</td>                            </tr>                            #set($ind = $ind + 1)                        #end                    </table>                </div>                #else                <div id="searchResult_1">                    <table width="100%" cellpadding="0" cellspacing="0" style="margin-top:1px;">                        <tr bgcolor="#efefef">                            <td class="check_box_top" width="17px"><input type="checkbox"/></td>                            <td class="box_subheader" style="border-right:none;"><span style="margin-left:4px;">$brander.getBrandElement("lms.manageUser.searchLearner.caption.Courses")</span></td>                        </tr>                    </table>                    <div class="result_blank">$brander.getBrandElement("lms.administrator.searchMember.caption.nocourseassociatedsurvey")</div>                </div>                #end            </div>            			<!-- button-div -->			<input type="hidden" id="currentpage" name="_page" value="0" />            <input type="hidden" id="wizardSubmitter" name="wizardSubmitter" />			<div class="managerBtn" style="position:static;">                <div style="float:right; margin-top:5px; margin-right:5px;" >                    <div class="btn_start"></div>                     <div class="btn_normal" onClick="doFinish(); submitButtonForm(document.searchCoursesForm,'wizardSubmitter','_finish');"><span class="button">$brander.getBrandElement("lms.Instructor.button.Finish")</span></div>                    <div class="btn_end"></div>                </div>                <div style="float:left; margin-top:5px; margin-left:15px;" >                    <div class="btn_start"></div>                     <div class="btn_normal" onClick="submitButtonForm(document.searchCoursesForm,'wizardSubmitter','_cancel');"><span class="button_wizard">$brander.getBrandElement("lms.Instructor.button.Cancel")</span></div>                    <div class="btn_end"></div>                </div>            </div>            <!-- button-div -->				</div>	</td></tr>		<tr> <!-- Footer BLOCK -->	<td id="footer" colspan="2">	#parse("snippets/adminFooter.vm")	</td></tr>   </table></form></body></html>