<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head>#parse("snippets/learnerNewHead.vm")<script type="text/javascript" src="javascript/jquery-1.2.6.min.js"></script><script type="text/javascript" language="javascript" src="javascript/js_manager.js"></script><script type='text/javascript' src='javascript/jquery.simplemodal.js' ></script><script type='text/javascript' src='javascript/js_survey.js' ></script><script type="text/javascript" language="javascript" > $(document).ready(function() {		$('#sideBar').height($('.scrollable').height());    });	 function confirm(message, callback) {        $('#deleteConfirmation').modal({            close:false,            position: ["40%",],            overlayId:'confirmModalOverlay',            containerId:'confirmModalContainer',            onShow: function (dialog) {                dialog.data.find('.message').append(message);                // if the user clicks "yes"                dialog.data.find('.yes').click(function () {                    // call the callback                    if (jQuery.isFunction(callback)) {                        callback.apply();                    }                    // close the dialog                    jQuery.modal.close();                });            }        });    }	 function deleted(){        if ($("INPUT[type='checkbox'][name='row']:checked").length>0) {            confirm("$brander.getBrandElement("lms.manageUser.manageLearnerGroup.caption.manageLearnerGroup.deleteConfirm")", function () {                document.getElementById('actionmethod').value='deleteCourse';                document.suggestedTrainingForm.submit();            });        }else {            $('#selectLearner').modal({close:false,position: ["40%",]});        }    }</script></head><body><form name="suggestedTrainingForm" method="post" action="adm_suggestedTraining.do"><input type="hidden" id="actionmethod" name="method" value=""/><table id="table-layout" border="0" cellpadding="0" cellspacing="0"> <!-- Table 1 -->	<tr> <!-- First Block -->		<td id="header" border="0" cellpadding="0" cellspacing="0" colspan="2">    <!--Header Start -->    #if($userData.isLMSAdministrator() && $adminCurrentMode == 'ROLE_LMSADMINISTRATOR')       #parse("snippets/adminHeader.vm")    #else       #parse("snippets/managerHeader.vm")    #end    <!-- Header End -->	</td></tr>	    <!-- Tabs Start -->   <td  id="row-tabs" class="tab_bar" colspan="2">        <table width="100%" cellpadding="0" cellspacing="0" border="0">            <tr>                <td width="194"><div class="tab_blank"></div></td>                <td>&nbsp;</td>                #springBind("suggTrainingForm.sid")                <td width="556">                     <a href="adm_manageSyrvey.do?method=showSurveyInfoView&sid=$!{status.value}"><div class="tab">$brander.getBrandElement("lms.assignEnrollmentSummary.caption.header")</div></a>                     <a href="adm_manageSyrvey.do?method=showQuestionView&sid=$!{status.value}"><div class="tab">$brander.getBrandElement("lms.listServeyQuestions.caption.Question")</div></a>					#if(!$context.eventType.equals("survey.event.manual") )                     <a href="adm_manageSyrvey.do?method=showCoursesView&sid=$!{status.value}"><div class="tab">$brander.getBrandElement("lms.manageUser.searchLearner.caption.Courses")</div></a>					#end                     <!-- commented as per discussion with faisal <a href="adm_manageSyrvey.do?method=showManageFlags&sid=$!{status.value}"><div class="tab">$brander.getBrandElement("lms.administrator.searchMember.caption.flags")</div></a>-->                     <a href="adm_suggestedTraining.do?sid=$!{status.value}"><div class="tab_selected" style="border-right-style:solid; width:140px;">$brander.getBrandElement("lms.administrator.searchMember.caption.suggestedtraining")</div></a>                </td>                <td>&nbsp;</td>            </tr>        </table>    </td>    <!-- Tabs End -->    <!-- Body Start --><tr id="main"> <!-- SECOND BLOCK -->	<td id="left-navigation" valign="top">		<table id="table-bands" border="0" cellpadding="0" cellspacing="0"> <!-- Table 3 -->    #parse("snippets/compliance/complianceLeftMenu.vm")	</table></td>        		<TD id="content" >	<div id="scrollable">            <!-- Error -->			#springBind("suggTrainingForm.*")                        #if($status.error)            <div id="message_box">                                <img id="close_message" style="float:right;cursor:pointer"  src="$brander.getBrandElement("lms.errorMessage.pixel.12-em-cross.png")" />                                #foreach($errorCode in $status.errorCodes)                                        #set ($error = $brander.getBrandElement($errorCode))                                        <div class="width40pct">                                                <div class="message_box_align"><img src="$brander.getBrandElement("lms.errorMessage.pixel.addLearner_06.gif")" width="14" height="14" alt="" /></div>                                                <div class="message_box_align">$error</div>                                        </div>                                #end                        </div>                        <!-- error-box -->                        #end           <!-- Logo+Manage-User -->					<table id="table-page-description" border="0" cellpadding="0" cellspacing="0" style="margin-top:15px;margin-bottom:15px">						    <tr>							  <td id="page-icon" class="icon_surveys">						            &nbsp;						        </td>						        <td id="page-description">						            <div id="page-heading">Edit Suggested Training</div>						            <div id="page-help">$brander.getBrandElement("lms.manageSurvey.manageSuggestedTraining.textDescription")</div>						        </td></tr></table>		            <!-- Logo+Manage-User-end -->            <div class="search_box_result" style="margin-left:20px;">                <div id="resultHeader" class="box_header">                    <!-- Result Heading -->                    <div style="float:left; height:23px;">					<div style="float:left; margin-top:3px;">					 #springBind("suggTrainingForm.sid")					  <a onClick="window.location.href='adm_addSuggestedTraining.do?sid=$!{status.value}';" style="text-decoration:none;">						  <div class="btn_start"></div>						  <div class="btn_normal">Add Courses</div>						  <div class="btn_end"></div>					  </a>					</div>										<div style="float:left; margin-top:3px;">					  <a onClick="deleted();" style="text-decoration:none;">						  <div class="btn_start"></div>						  <div class="btn_normal">Delete Courses</div>						  <div class="btn_end"></div>					  </a>					</div>										#springBind("suggTrainingForm.suggTraining")					#set($Training = $!{status.value})					<div style="float:left; margin-top:3px;">					  <a href="adm_suggestedTraining.do?method=editSuggestedTraining&id=$!Training.id" style="text-decoration:none;">						  <div class="btn_start"></div>						  <div class="btn_normal">Assign Multiple Courses</div>						  <div class="btn_end"></div>					  </a>					</div>					                                          </div>                </div>                #set($dex = 0)                #set($sTraining = $context.courses)                #set($courseNumber = $sTraining.size() - 1)                <div id="searchResult">                    <table width="100%" cellpadding="0" cellspacing="0" style="margin-top:1px;">                        <tr bgcolor="#efefef">                            <td class="check_box_top" width="17px" #if($sTraining.size()) style="border-bottom:none"; #end><input type="checkbox" name="selectAll" onClick="checkAll(document.myform.row, this)" style="margin-left:-2px"/></td>                            <td class="box_subheader" #if($sTraining.size()) style="border-bottom:none;border-right:none;"; #end><span style="margin-left:4px;">Courses</span></td>                        </tr>                        #foreach($sTran in $sTraining)                            #if($dex%2 == 0)                                #set($rowIndex="row_1")                            #else                                #set($rowIndex="row_2")                            #end                            <tr>                                <td class="$rowIndex" #if($courseNumber==$dex) style="border-bottom:none; width=17px" #end><input type="checkbox" value="$sTran.id"  name="row" style="margin-left:-2px"/></td>                                <td class="$rowIndex" #if($courseNumber==$dex) style="border-bottom:none; width=17px" #end>                                     <!--<a href="adm_suggestedTraining.do?method=editSuggestedTraining&id=$sTran.id"><u> -->                                                                                   $!{sTran.courseTitle}                                                                            <!-- </u></a> -->                                </td>                            </tr>                            #set($dex = $dex + 1)                        #end                    </table>                </div>                            </div>        </div>    </td></tr>	<tr> <!-- Footer BLOCK -->	<td id="footer" colspan="2">	#if($userData.isLMSAdministrator() && $adminCurrentMode == 'ROLE_LMSADMINISTRATOR')       #parse("snippets/adminFooter.vm")    #else        #parse("snippets/managerFooter.vm")    #end    <!-- Body End -->	</td></tr>    <!-- Footer Start -->       <!-- Footer End --></table><div id="selectLearner" style="display:none" class="message_box">			<div class="alert_message_box_title">$brander.getBrandElement("lms.accreditation.caption.Warning")</div>			<div class="alert_message_box_icon"></div>			<div class="alert_message_box_text">$brander.getBrandElement("lms.accreditation.caption.Please-select-0ne")</div>			<center>				<div id="btn_ok_select" style="margin-left:132px; margin-top:20px;" class="simplemodal-close">					<div class="btn_normal">$brander.getBrandElement("lms.accreditation.caption.OK")</div><div class="btn_end"></div>				</div>			</center>		</div>		<div id='deleteConfirmation'>			<div class='header' style="width:464px;"><span>$brander.getBrandElement("lms.accreditation.caption.Please-Confirm")</span></div>			<div class="alert_message_box_icon"></div>			<div class="alert_message_box_text">Are you sure you want to delete the selected Course(s)?</div>			<br style="clear:both" />			<div class='buttons'>				<div class='simplemodal-close'>					<a class="button" onClick=""><span class="no button">$brander.getBrandElement("lms.accreditation.caption.Cancel")</span></a>					<a class="button" onClick=""><span class="yes button">$brander.getBrandElement("lms.accreditation.caption.OK")</span></a>				</div>			</div>		</div></form></div></html>