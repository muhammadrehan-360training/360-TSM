rorResposeValue(para){ 	   		if($(para).attr("checked") == true){		   document.getElementById('rememberPriorResponse').value='true';		}else{		   document.getElementById('rememberPriorResponse').value='false';		}	}	function setAnonymousValue(para){ 	   		if($(para).attr("checked") == true){		   document.getElementById('allowAnonymousResponse').value='true';		}else{		   document.getElementById('allowAnonymousResponse').value='false';		}	}	function linksReqiured(para){		if($(para).attr("checked") == true){			$("#links-required").show();			$("#spaceDiv2").hide();			document.getElementById('links').value='true';		}else{			$("#links-required").hide();			$("#spaceDiv2").show();			document.getElementById('links').value='false';		}	}	function setEditable(para){		if($(para).attr("checked") == true){			document.getElementById('editable').value='true';		}else{			document.getElementById('editable').value='false';		}	}	 #springBind("manageSurveyForm.allQuestionPerPage")	 $(document).ready(function(){	#if(${status.value} == "false")	 document.getElementById('show').style.display = "block";	 #end  });	function show(){     document.getElementById('show').style.display = "block";     }	function hide(){     document.getElementById('show').style.display = "none";     }</script></head><body> <form name="editSurveyInfoForm" method="POST" action="adm_manageSyrvey.do">  <input type="hidden" id="actionmethod" name="method" value=""/><table id="table-layout" border="0" cellpadding="0" cellspacing="0"> <!-- Table 1 -->	<tr> <!-- First Block -->		<td id="header" border="0" cellpadding="0" cellspacing="0" colspan="2">    <!--Header Start -->	#if($manageSurveyForm.sid != 0)    #parse("snippets/adminHeader.vm")	#else		#parse("snippets/adminWizardHeader.vm")#end    <!-- Header End -->	</td></tr>	       #if($manageSurveyForm.sid != 0)       <td  id="row-tabs" class="tab_bar" colspan="2">		 <table width="100%" cellpadding="0" c