document.getElementById('validateVariable').value='donotValidate';
document.editCustomerForm.submit();
}

    $(document).ready(function(){
        $('#content2a').height($(window).height() - ($('#header').height()+$('#footer').height()));
        if ($('#right-pannel').height()<$('#content2a').height()) {
            $('#right-pannel').height($('#content2a').height());
        }
        $('#sideBar').height($('#right-pannel').height());
        $('#content2a').scroll(function()    {
            $('#message_box').animate({top:$('#content2a').scrollTop()+"px" },{queue: false, duration: 350});  
        });
        $('#close_message').click(function()    {
            $('#message_box').animate({ top:"+=15px",opacity:0 }, "slow");
        });
    });
    function FormSubmit(buttonclick) {
        document.getElementById('actionmethod').value='saveCustomerProfile';
        document.editCustomerForm.submit();
    }
    function EnableLCMS(buttonclick) {
        document.getElementById('actionmethod').value=buttonclick;
        document.editCustomerForm.submit();
    }
    function EditCancel(buttonclick) {
        document.getElementById('actionmethod').value='cancelEditCustomer';
        document.editCustomerForm.submit();
    }
    function vlidateChange(ob){
        if ((ob.value=="b2c") || (ob.value=="B2C")){
            $('#b2cSelect').modal({close:false,position: ["40%",]});
            ob.options[0].selected = true;
            return false;
        }
    }
</script>
