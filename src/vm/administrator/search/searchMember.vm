		function doSearchLearner() {
		
		   chkLearner = document.getElementById('constrainedLearnerSearch');
		   
			var firstName=  trim( ltrim( rtrim( document.searchLearnerForm.searchFirstName.value ) ) ) ;
 			var lastName= trim( ltrim( rtrim( document.searchLearnerForm.searchLastName.value ) ) ) ;
			var email= trim( ltrim( rtrim( document.searchLearnerForm.searchEmailAddress.value ) ) );
			
			//LMS-22049
			//firstName = replaceAll(firstName, "%" , "");
 			//lastName = replaceAll(lastName, "%" , "");
 			
			if(firstName.length != 0 && firstName.length < 2 ){
				return false;
			}
			else if(lastName.length != 0 && lastName.length < 2 ) {
				return false;
			}
			
			if(chkLearner != null && chkLearner.checked){
        		// do nothing 
			}else if( firstName.length == 0 &&  lastName.length == 0  &&  email.length == 0 ){
				$('#selectLearner').modal({close:false,position: ["40%",]});
				return false;
			}
			var selectedResultSize=$('#resultSizeDropDown :selected').text();
 			$("#searchLearnerForm #selectedResultSize").val(selectedResultSize);
			document.searchLearnerForm.submit();
        }
        
        function doSearchCustomer(submitFormName) {
        	chkSimple = document.getElementById('constrainedCustomerSearchSimple');
        	chkAdvance = document.getElementById('constrainedCustomerSearchAdvance');
        	
        	var firstName, orderId, email, date;
        	if(submitFormName == 'searchCustomerForm') {
        		firstName = trim(ltrim(rtrim(document.searchCustomerForm.searchCustomerName.value)));
        		orderId = trim(ltrim(rtrim(document.searchCustomerForm.searchOrderId.value)));
        		email = trim(ltrim(rtrim(document.searchCustomerForm.searchCusEmailAddress.value)));
        		date = trim(ltrim(rtrim(document.searchCustomerForm.searchOrderDate.value)));
        	} else if(submitFormName == 'advanceSearchCustomerForm') {
        		firstName = trim(ltrim(rtrim(document.advanceSearchCustomerForm.searchCustomerName.value)));
        		orderId = trim(ltrim(rtrim(document.advanceSearchCustomerForm.searchOrderId.value)));
        		email = trim(ltrim(rtrim(document.advanceSearchCustomerForm.searchCusEmailAddress.value)));
        		date = trim(ltrim(rtrim(document.advanceSearchCustomerForm.searchOrderDate.value)));
        	}
        	
        	
        	if(chkSimple != null && chkAdvance != null && (chkSimple.checked || chkAdvance.checked)){
        		// do nothing 
        	}else if(firstName.length == 0 && orderId.length == 0 && email.length == 0 && date.length == 0) {
        		$('#selectLearner').modal({close:false,position: ["40%",]});
				return false;
        	}
        	if(submitFormName == 'searchCustomerForm') {
        		var selectedResultSize=$('#resultSizeDropDown :selected').text();
 				$("#searchCustomerForm #selectedResultSize").val(selectedResultSize);
        		document.searchCustomerForm.submit();
        	} else if(submitFormName == 'advanceSearchCustomerForm') {
        		var selectedResultSize=$('#resultSizeDropDown :selected').text();
 				$("#advanceSearchCustomerForm #selectedResultSize").val(selectedResultSize);
        		document.advanceSearchCustomerForm.submit();
        	}
        }

	var sortIndex;
    function onAllPage(){
        var sortIndex=document.getElementById('sortColumnIndex').value;
    	document.getElementById('sortIndex').value=sortIndex;
    	document.getElementById('direction').value=1;
        document.getElementById('actionmethod').value='refreshQuestions';
        document.getElementById('pageIndex').value=0;
        document.getElementById('action').value="paging";
        document.getElementById('showAll').value="true";
        document.nextPreviousForm.submit();
    }

	
    function onNextPage(nextIndex){
        var sortIndex=document.getElementById('sortColumnIndex').value;
    	document.getElementById('sortIndex').value=sortIndex;
    	var direction=document.getElementById('sortDirection').value;
    	document.getElementById('direction').value=direction;
    	document.getElementById('actionmethod').value='refreshQuestions';
        document.getElementById('pageIndex').value=nextIndex;
        document.getElementById('action').value="paging";
        document.getElementById('showAll').value="false";
        var itemNo= document.getElementById('pageCurrIndex').value;
        var items=$pager.itemsPerPage;
        document.getElementById('pageCurrIndex').value=parseInt(items) +  parseInt(itemNo);
        document.nextPreviousForm.submit();
    }

    function onPreviousPage(previousIndex){
        var sortIndex=document.getElementById('sortColumnIndex').value;
    	document.getElementById('sortIndex').value=sortIndex;
    	var direction=document.getElementById('sortDirection').value;
    	document.getElementById('direction').value=direction;
        document.getElementById('actionmethod').value='refreshQuestions';
        document.getElementById('pageIndex').value=previousIndex;
        document.getElementById('action').value="paging";
        document.getElementById('showAll').value="false";
        var itemNo= document.getElementById('pageCurrIndex').value
        var items=$pager.itemsPerPage;
        document.getElementById('pageCurrIndex').value= parseInt(itemNo) - parseInt(items) ;
        document.nextPreviousForm.submit();
    }

    function sortLearner(sortColumnIndex, sortDirection, showAll){
        document.getElementById('lastActionmethod').value='searchMembers';
        document.getElementById('lastShowAll').value=showAll;
        document.getElementById('sortColumnIndex').value=sortColumnIndex;
        document.getElementById('sortDirection').value=sortDirection;
        document.searchResultForm.submit();
    }
    
    function initialize(ob){
