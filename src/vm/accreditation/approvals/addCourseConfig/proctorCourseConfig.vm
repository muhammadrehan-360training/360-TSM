				//$('#pvf_requireProctorValidation').attr('disabled', false);
				//$('#lvt_requireLearnerValidation').attr('disabled', false);
				//$('#lvf_requireLearnerValidation').attr('disabled', false);
			}
			
			
            
             
        });
    </script>
</head>
<body>
<form id="addCourseConfigForm" name="addCourseConfigForm" method="post" action="acc_addCourseConfig.do" >
  <table id="table-layout" border="0" cellpadding="0" cellspacing="0"> <!-- Table 1 -->
