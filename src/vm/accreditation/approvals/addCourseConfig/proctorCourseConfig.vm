				//$('input:radio[name=requireNyInsurance]:nth(0)').attr('checked', false);
				//$('input:radio[name=requireNyInsurance]:nth(1)').attr('checked', true);
				//$('input:radio[name=requireNyInsurance]:nth(0)').attr('disabled', true);

				$('input:radio[name=caRealEstateCE]:nth(0)').attr('checked', false);
				$('input:radio[name=caRealEstateCE]:nth(1)').attr('checked', true);
				$('input:radio[name=caRealEstateCE]:nth(0)').attr('disabled', true);

				//$('#pvt_requireProctorValidation').attr('checked', false);
				//$('#pvf_requireProctorValidation').attr('checked', true);
				//$('#pvt_requireProctorValidation').attr('disabled', false);
				//$('#pvf_requireProctorValidation').attr('disabled', false);
				
				if ($("#pvr").length > 0) $("#pvr").remove();
	
			});

			// incase of both validation off then disable=false 			
			var pv = $('input:radio[name=requireProctorValidation]:checked').val();
			var lv = $('input:radio[name=requireLearnerValidation]:checked').val();
			if( pv == 'false' && lv =='false' ){
				##alert('Both shoul be off');
