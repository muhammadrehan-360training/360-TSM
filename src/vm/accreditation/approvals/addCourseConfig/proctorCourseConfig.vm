                            <div style="" class="btn_radio3">&nbsp;&nbsp;
                            	<input type="radio" id="" name="proctorValidatorName" value="nyInsurance" #if(${status.value}=="nyInsurance") checked #end #if(${requireProctorVal}=="false") disabled #end /> 
