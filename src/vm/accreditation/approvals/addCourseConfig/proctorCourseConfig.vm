                        <td></td>
					</tr>
					<tr>
    					<td height="5"></td>
						<td></td>
						<td></td>
                        <td></td>
					</tr>
					<tr>
						<td></td>
						<td>
							#springBind("courseConfigForm.requireLearnerValidation")
							#if($status.value)
	    						#springBind("courseConfigForm.caRealEstateCE")
	                            <div class="btn_radio3">&nbsp;&nbsp;<input type="radio" tabindex="23" id="${status.expression}" name="${status.expression}" value="true" #if(${status.value}=="true") checked #end  /> $brander.getBrandElement("lms.accraditation.courseConfiguation.caption.caRealEstateCE")</div>
	                            <input style="visibility:hidden;" type="radio" tabindex="23" id="h_${status.expression}" name="${status.expression}" value="false" />
							#else
	    						#springBind("courseConfigForm.caRealEstateCE")
	                            <div class="btn_radio3">&nbsp;&nbsp;<input type="radio" tabindex="23" id="${status.expression}" name="${status.expression}" value="true" disabled  /> $brander.getBrandElement("lms.accraditation.courseConfiguation.caption.caRealEstateCE")</div>
	                            <input style="visibility:hidden;" type="radio" tabindex="23" id="$h_{status.expression}" name="${status.expression}" value="false" checked />
							#end
						</td>
    					<td>
    						
    					</td>
                        <td></td>
					</tr>
					<tr>
    					<td height="5"></td>
						<td></td>
