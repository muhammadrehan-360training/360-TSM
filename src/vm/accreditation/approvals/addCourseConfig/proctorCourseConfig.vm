				$('input:radio[name=caRealEstateCE]:nth(0)').attr('disabled', true);
				
				if ($("#lvr").length > 0) $("#lvr").remove();
				
			});
			
			//Learner Policies - > Required Learner Validation - when ON
			$("#lvt_requireLearnerValidation").click( function(){

				$('#pvt_requireProctorValidation').attr('checked', false);
				//$('#pvt_requireProctorValidation').attr('disabled', true);
				$('#pvf_requireProctorValidation').attr('checked', true);
				//$('#pvf_requireProctorValidation').attr('disabled', true);

				$('input:radio[name=proctorValidatorName]:nth(0)').attr('disabled', true);
				$('input:radio[name=proctorValidatorName]:nth(1)').attr('disabled', true);
				
				$('#pvt_requireProctorValidation').attr('checked', false);
				$('#pvf_requireProctorValidation').attr('checked', true);
				
				//$('input:radio[name=requireNyInsurance]:nth(1)').attr('checked', true);
				//$('input:radio[name=requireNyInsurance]:nth(0)').attr('disabled', true);

				$('input:radio[name=caRealEstateCE]:nth(0)').attr('checked', true);
				$('input:radio[name=caRealEstateCE]:nth(0)').attr('disabled', false);

				/*
				No radio button field value transfer to action/form incase of disability
				to handle this issue, we have to generated field 
				so that it can initlization with new value. 
								
				if ($("#pvr").length > 0) $("#pvr").remove();
				if ($("#pvr").length <= 0){
					var generateHiddenField = '<input name="requireProctorValidation" type="hidden" value="false" id="pvr">'; 
					$("#pvf_requireProctorValidation").after(generateHiddenField);
				}*/
			});
			
			//Learner Policies - > Required Learner Validation - when OFF
			$("#lvf_requireLearnerValidation").click( function(){
				
				//$('input:radio[name=proctorValidatorName]:nth(0)').attr('checked', false);
