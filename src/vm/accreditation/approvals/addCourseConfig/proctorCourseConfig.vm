					$("#lvf_requireLearnerValidation").after(hiddenField);
				}*/
	
			});
			//Proctor Off fun binding.
			$("#pvf_requireProctorValidation").click(function(){
			
				$('input:radio[name=proctorValidatorName]:nth(0)').attr('checked', false);
				$('input:radio[name=proctorValidatorName]:nth(1)').attr('checked', false);
				$('input:radio[name=proctorValidatorName]:nth(0)').attr('disabled', true);
				$('input:radio[name=proctorValidatorName]:nth(1)').attr('disabled', true);
				
				//$('input:radio[name=requireNyInsurance]:nth(0)').attr('checked', false);
				//$('input:radio[name=requireNyInsurance]:nth(1)').attr('checked',true);
