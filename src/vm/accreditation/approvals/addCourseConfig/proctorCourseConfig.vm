				$('input:radio[name=requireAnsi]:nth(1)').attr('checked',true);
			});
			*/
			
			//Proctor On fun binding.
			$("#pvt_requireProctorValidation").click( function(){
			
			   	$('input:radio[name=proctorValidatorName]:nth(0)').attr('checked',true);
	        	$('input:radio[name=proctorValidatorName]:nth(1)').attr('checked',false);
	        	$('input:radio[name=proctorValidatorName]:nth(0)').attr('disabled', false);
	        	$('input:radio[name=proctorValidatorName]:nth(1)').attr('disabled', false);
				
				//$('input:radio[name=requireNyInsurance]:nth(0)').attr('disabled', false);
				//$('input:radio[name=requireNyInsurance]:nth(0)').attr('checked', false);
				//$('input:radio[name=requireNyInsurance]:nth(1)').attr('checked',true);
	
