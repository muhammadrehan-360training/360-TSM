					<tr>
    					<td height="5"></td>
						<td></td>
						<td></td>
                        <td></td>
					</tr>
					<tr>
						<td></td>
						<td>
							#springBind("courseConfigForm.requireProctorValidation")
							#set ($requireProctorVal=${status.value})
							
    						#springBind("courseConfigForm.proctorValidatorName")
                            <div class="btn_radio3">&nbsp;&nbsp;
                            	<input type="radio" tabindex="23" id="" name="proctorValidatorName" value="ansi" #if(${status.value}=="ansi") checked #end  #if(${requireProctorVal}=="false") disabled #end/> 
                            	$brander.getBrandElement("lms.accraditation.courseConfiguation.caption.ansi")
