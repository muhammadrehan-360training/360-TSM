<head>
#parse("snippets/accreditation/accreditationDefaultHead.vm")
   
    <script type="text/javascript" src="javascript/jquery.alphanumeric.pack.js"></script>
	<script src="javascript/360js.js" language="javascript" type="text/javascript" ></script>
    <script type="text/javascript" language="javascript">
         $(document).ready(function(){
            $('#content2a').scroll(function(){
            $('#message_box').animate({top:$('#content2a').scrollTop()+"px" },{queue: false, duration: 350});  
            });
            $('#close_message').click(function(){
                $("#message_box").animate({
                   opacity: .5
                }, "slow", "linear", function(){$("#message_box").hide();} );
            });
            $("INPUT[type='checkbox'][name='chk']").click(
               function(){
                  $(this).parent().find("INPUT[type='hidden']").attr('value', $(this).is(':checked'));
               }
            );
            $('.numeric').numeric({allow:"."});
        });
		
		$('#enableIdentityValidation').change(function(){                                
                var status = $(this).is(':checked') ? false : true;
                $('#enableSmartProfileValidation').attr('disabled', status);
               	$('#enableSmartProfileValidation').trigger('change');               
            });
        $('#enableSmartProfileValidation').change(function(){                                
                var status = false;
                if($(this).is(':checked') || !$('#enableIdentityValidation').is(':checked'))	{
                	status = true;
            	}
               	$('#validationTimeBetweenQuestion').attr('disabled', status);
               	$('#allowSecondsToAnswerEachQuestion').attr('disabled', status);
               	$('#validationNoMissedQuestionsAllowed').attr('disabled', status);                
            });
            $('#enableIdentityValidation').trigger('change');
            $('#enableSmartProfileValidation').trigger('change');
            
         });
		
    </script>
</head>
