					</tr>
					
					
					<tr>
                        <td></td>
						<td>
							#springBind("courseConfigForm.seattimeinhour")
							<div class="row_text">Seat Time <input type="text" class="input_text_inline numeric" id="$!{status.expression}" name="${status.expression}" value=#if($status.value<10) "0${status.value}" #else "$!{status.value}" #end style="width:30px;"/> Hour(s)
						
								#springBind("courseConfigForm.seattimeinmin")
								<input type="text" class="input_text_inline numeric" id="$!{status.expression}" name="${status.expression}" value=#if($status.value<10) "0${status.value}" #else "$!{status.value}" #end style="width:30px;"/> Minute(s)
							</div>
