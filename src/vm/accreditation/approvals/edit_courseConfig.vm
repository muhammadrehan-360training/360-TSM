	}
}

function unlinkCertificate(){

	if($('#certificateEnabled').is(':checked')){
		document.editCourseConfigForm.action="acc_manageCourseConfig.do?method=unlinkCertificate";
		document.editCourseConfigForm.submit();
	}
}



            function confirm(message, callback) {
                $('#deleteConfirmation').modal({
                    close:false,
                    position: ["40%",],
                    overlayId:'confirmModalOverlay',
                    containerId:'confirmModalContainer', 
                    onShow: function (dialog) {
                        dialog.data.find('.message_box_text').append(message);

                        // if the user clicks "yes"
                        dialog.data.find('.yes').click(function () {
                            // call the callback
                            if (jQuery.isFunction(callback)) {
                                callback.apply();
                            }
                            // close the dialog
                            jQuery.modal.close();
                        });
                    }
                });
            }

            function publishCourseConfig(){
				var confirmValidationQuestions = function() { 
        						confirm("Are you sure you want to publish this course configuration? This may disturb the live user(s).", function () {					
									setProperties();
									var form = document.editCourseConfigForm;
			                        form.action = "acc_manageCourseConfig.do?method=saveAndPublish"; 
			                        form.submit();
			                    });
            				}
            	if ($('#enableIdentityValidation').is(':checked') && $('#enableDefineUniqueQuestionValidation').prev().is(':checked')) {
				    if (hasblankUniqueQuestion()) {
				        custom_alert1("Blank Unique Questions are not allowed");
				    } else {
				        confirmValidationQuestions();
				    }
				} else {
				    confirmValidationQuestions();
				}
            }
</script>
