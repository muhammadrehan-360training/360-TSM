		function hasblankUniqueQuestion() {
  			if($("#dataTable tr input:text[name*=uquestionName]").length == 0) return true;
  			if($("#dataTable tr input:text[name*=uquestionName][value='']").length > 0) return true;
  			return false;
		}
		
		function deleteRow(tableID) {
			try {
			var table = document.getElementById(tableID);
			var rowCount = table.rows.length;
            var id = null;
            //var deletedIds = new Array(table.rows.length)
            var deletedIds = " ";
			for(var i=1; i<rowCount; i++) {
				var row = table.rows[i];
	            var deletedIds;
				var chkbox = table.rows[i].cells[0].children[0];
				
				if(null != chkbox && true == chkbox.checked) {
				 //console.log(chkbox.value);
                   
                    deletedIds = deletedIds + chkbox.value + ',';
					//deletedIds[i] = chkbox.value;
					table.deleteRow(i);
					rowCount--;
					i--;
				}
			}
			if(deletedIds!=null){

			     //document.getElementById('actionmethod').value='deleteQuestion';
			     document.getElementById('uquestionDeleteId').value = deletedIds;
			     //document.editCourseConfigForm.submit();
			}
			}catch(e) {
 				alert(e);
  			}
			}
			
function requireProctorValidationChange(){
	return;
	
	if($('#chk_requireProctorValidation').is(':checked')){
		$('#chk_preAllowPauseResumeAssessment').attr('disabled', true);
		$('#chk_quizAllowPauseResumeAssessment').attr('disabled', true);
		$('#chk_postAllowPauseResumeAssessment').attr('disabled', true);
	}else{
		$('#chk_preAllowPauseResumeAssessment').attr('disabled', !$('#preAssessmentEnabled').is(':checked'));
		$('#chk_quizAllowPauseResumeAssessment').attr('disabled', !$('#quizAssessmentEnabled').is(':checked'));
		$('#chk_postAllowPauseResumeAssessment').attr('disabled', !$('#postAssessmentEnabled').is(':checked'));
	}
}

function setRequireProctorField(){
	return;
	if($('#chk_preAllowPauseResumeAssessment').is(':checked') || $('#chk_quizAllowPauseResumeAssessment').is(':checked') || $('#chk_postAllowPauseResumeAssessment').is(':checked')){
		$('#chk_requireProctorValidation').attr('disabled', true);
	}else{
		$('#chk_requireProctorValidation').attr('disabled', false);
	}

}

function setControls(){
	var form = document.editCourseConfigForm;
	var IsChecked = document.getElementById('mustStartWithinAfterReg').checked;
	form.mustStartCourseWithinSpecifiedAmountOfTimeAfterRegistration.disabled = !IsChecked;
	form.mustStartCourseWithinSpecifiedAmountOfTimeAfterRegistrationUnit.disabled = !IsChecked;
	setRequireProctorField();
	requireProctorValidationChange();	
}

function custom_alert1(output_msg, title_msg)
        {
           $('#alertMessage').modal({
                    close:false,
                    position: ["40%",],
                    overlayId:'confirmModalOverlay',
                    containerId:'confirmModalContainer', 
                    onShow: function (dialog) {
                        dialog.data.find('.message_box_text').append(output_msg);
                    // if the user clicks "yes"
                        dialog.data.find('.yes').click(function () {
                            // call the callback
                            if (jQuery.isFunction(callback)) {
                                callback.apply();
                            }
                            // close the dialog
                            jQuery.modal.close();
                        });
                    }
                });
}			
       
       
       function saveCourseConfig() {
            
       if ($('#enableIdentityValidation').is(':checked') && $('#enableDefineUniqueQuestionValidation').prev().is(':checked')) {
			    if (hasblankUniqueQuestion()) {
			        custom_alert1("Blank Unique Questions are not allowed");
			    } else {
			        document.getElementById('actionmethod').value = 'saveCourseConfig';
			        document.editCourseConfigForm.submit();
			    }
			} else {
			    document.getElementById('actionmethod').value = 'saveCourseConfig';
			    document.editCourseConfigForm.submit();
			}
	    }

