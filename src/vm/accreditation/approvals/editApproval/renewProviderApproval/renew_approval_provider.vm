                            #elseif(! $status.value || $status.value.equals(""))<div class="required_field">*</div>#else <div class="required_field">*</div>#end<input type="text" id="${status.expression}" name="${status.expression}" value="$!{status.value}" date:closeAtTop="false" date:buttonText="$brander.getBrandElement("lms.administrator.updateEntitlements.calender.tooltipEndDate")" class="input_text_field" style="width:152px;" /><!-- <div class="icon_calen"></div> --></td>
                        <script>
                            $("#${status.expression}").datepicker({ 
                                showOn: "both", 
                                buttonImage: "$brander.getBrandElement("lms.pixel.iconCalenderGif")", 
                                buttonImageOnly: true 
                            });
                        </script>
					</tr>
					<tr>
    					<td><div class="row_text">$brander.getBrandElement("lms.accraditation.editApproval.caption.Provider-Approval-Status")</div></td>
                        #springBind("approvalForm.renewProviderApproval.providerApprovalStatus")
    					<td>#if($status.error)<div class="required_field"><img src="$brander.getBrandElement("lms.errorMessage.pixel.addLearner_06.gif")"></div>
                            #elseif(! $status.value || $status.value.equals(""))<div class="required_field">*</div>#else <div class="required_field">*</div>#end<input type="text" class="input_text_field" name="${status.expression}" value="$!{status.value}" /></td>
    					<td><div class="row_text">$brander.getBrandElement("lms.accraditation.editApproval.caption.Date-Provider-Submitted")</div></td>
