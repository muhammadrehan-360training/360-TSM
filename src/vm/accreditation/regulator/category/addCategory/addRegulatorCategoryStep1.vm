                    toggleView('toggleIconForCustomField', 'collapsibleCustomField');
                });
                $("#toggleAddrInfo").click(function() {
                    toggleView('toggleAddrInfo', 'collapsibleAddrInfo');
                });
            });

            function checkAnswer(objCheckBox){
                if(objCheckBox.checked){
                    document.getElementById('hdnChoice_'+objCheckBox.id).value="true";
                }else {
                    document.getElementById('hdnChoice_'+objCheckBox.id).value="false";
                }
            }

            function stateChangeFormSubmit(dropdown) {
                if (dropdown.name=="regulator.address.country")
                {          
                    document.getElementById('city1').value="";
                }
                document.getElementById('wizardSubmitter').name = '_target0';
                document.getElementById('validateVariable').value='false';
                document.regulatorCategoryForm.submit();
            }

            function setValidate() {            
                document.getElementById('validateVariable').value='true';
            }
		
            function showOtherOption(obj){		 	
                if(obj.value=="other"){               
                    document.getElementById("otherJurisdictionTr").style.visibility = "visible";
                }else{            
                    document.getElementById("otherJurisdictionTr").style.visibility = "hidden";
                }			
            }
