<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        #parse("snippets/accreditation/accreditationDefaultHead.vm")

        <script src="javascript/360js.js" language="javascript" type="text/javascript" ></script>
        <script language="javascript" type="text/javascript">
            $(document).ready(function(){
                $('#content2a').scroll(function(){
                    $('#message_box').animate({top:$('#content2a').scrollTop()+"px" },{queue: false, duration: 350});  
                });
                $('#close_message').click(function(){
                    $("#message_box").animate({
                        opacity: .5
                    }, "slow", "linear", function(){$("#message_box").hide();} );
                });
			
                $("#toggleIconSummary").click(function() {
                    toggleView('toggleIconSummary', 'collapsibleSummary');
                });
            });
        </script>
    </head>

    <body>
        <form name="regulatorForm" method="post" action="acc_editReg_addDoc.do">
            <table id="table-layout" border="0" cellpadding="0" cellspacing="0"> <!-- Table 1 -->
                <tr> <!-- First Block -->
                    <td id="header" border="0" cellpadding="0" cellspacing="0" colspan="2">
                        <!-- Header starts -->
                        #parse("snippets/managerWizardHeader.vm")
                        <!-- Header ends -->
                    </td></tr>

                <!-- Body Start -->
                <tr id="main"> <!-- SECOND BLOCK -->
                    <td id="left-sidebar" valign="top">
                        <table id="table-bands" border="0" cellpadding="0" cellspacing="0"> <!-- Table 3 -->
                            <div>
                                <div id="l-menu-back">
                                    <div class="l-menu-back-text-h">$brander.getBrandElement("lms.accraditation.document.caption.addDocument.title")<br>$brander.getBrandElement("lms.accraditation.editRegulator.addContact.leftmenu.showing2of2")</div>
                                    <div class="l-menu-back-text">$brander.getBrandElement("lms.accraditation.editRegulator.addContact.leftmenu.summary")</div>
                                    <div class="l-menu-back-text-act">$brander.getBrandElement("lms.accraditation.editRegulator.addContact.leftmenu.confirmation")</div>
                                </div>
                            </div>
                        </table></td>

                    <TD id="content" >
                        <div id="scrollable">
                            <!-- Page Description -->
                            <table id="table-page-description" border="0" cellpadding="0" cellspacing="0" style="margin-top:15px;margin-bottom:15px">
                                <tr><td id="page-icon" class="icon_regulator">&nbsp;</td>
                                    <td id="page-description">
                                        <div id="page-heading">$brander.getBrandElement("lms.accraditation.document.caption.addDocument")</div>
                                        <div id="page-help">$brander.getBrandElement("lms.accraditation.manageRegulator.addDocument.descriptionStep2")</div>
                                    </td>
                                </tr>
                            </table>	

                            <div class="search_box_result">
                                <div class="box_header">
                                    <div id="toggleIconSummary" style="float: left; margin-right: 5px;" class="icon-caret-down-large"></div>
                                    <span class="box_heading">Summary</span>
                                </div>
                                <div id="collapsibleSummary">
                                    <table border="0" width="100%" cellpadding="0" cellspacing="3" heigth="100%">
                                        <tr>
                                            <td width="20%"><span class="row_text"><b>$brander.getBrandElement("lms.accraditation.addDocument.caption.file")</b></span></td>
                                            #springBind("documentUploadForm.fileName")
                                            <td><span class="row_text">$!{status.value}</span></td>
                                            <td></td>
                                            <td></td>
                                        </tr>
                                        <tr>
                                            <td width="20%"><span class="row_text"><b>$brander.getBrandElement("lms.accraditation.editRegulator.document.caption.name")</b></span></td>
                                            #springBind("documentUploadForm.document.name")
                                            <td><span class="row_text">$!{status.value}</span></td>
                                            <td></td>
                                            <td></td>
                                        </tr>
                                        <tr>
                                            <td width="20%" valign="top"><span class="row_text"><b>$brander.getBrandElement("lms.accraditation.editRegulator.document.caption.description")</b></span></td>
                                            #springBind("documentUploadForm.document.description")
                                            <td><span class="row_text">$!{status.value}</span></td>
                                            <td></td>
                                            <td></td>
                                        </tr>
                                        
                                    </table>
                                </div>
                            </div>

                            <input type="hidden" id="currentpage" name="_page" value="1" />
                            <input type="hidden" id="wizardSubmitter" name="wizardSubmitter" />

                            <!-- ************************************************* -->
                            <div class="btn-div">
                                <div style="float:right; margin-top:15px;">
                                    <a onClick="submitButtonForm(document.regulatorForm,'wizardSubmitter','_finish');">
                                        <div class="btn_start"></div>
                                        <div class="btn_normal">$brander.getBrandElement("lms.accraditation.addRegulator.caption.Finish")</div>
                                        <div class="btn_end"></div>
                                    </a>
                                </div>	

                                <div style="float:right; margin-top:15px;margin-right:5px;">
                                    <a onClick="submitButtonForm(document.regulatorForm,'wizardSubmitter','_target0');">
                                        <div class="btn_start"></div>
                                        <div class="btn_normal">$brander.getBrandElement("lms.accraditation.addRegulator.caption.Previous")</div>
                                        <div class="btn_end"></div>
                                    </a>
                                </div>

                                <div style="float:left; margin-top:15px;margin-left:10px;">
                                    <a onClick="submitButtonForm(document.regulatorForm,'wizardSubmitter','_cancel');">
                                        <div class="btn_start"></div>
                                        <div class="btn_normal">$brander.getBrandElement("lms.accraditation.addRegulator.caption.Cancel")</div>
                                        <div class="btn_end"></div>
                                    </a>
                                </div>  
                            </div>	

                            <!-- bottom-space -->
                            <div class="manage-bottom-space"></div>
                            <!-- bottom-space -->

                        </div>
                    </td></tr>
                <!-- Body End -->
                <tr> <!-- Footer BLOCK -->
                    <td id="footer" colspan="2">
                        <!-- Footer Start -->
		    #parse("snippets/managerFooter.vm")
                        <!-- Footer End -->
                    </td></tr>
            </table>
        </form>
    </body>
</html>