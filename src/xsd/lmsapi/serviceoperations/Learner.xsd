<?xml version="1.0" encoding="UTF-8"?>

<!-- Chameleon Namespace Design:
		This design approach says to give the "main" schema a targetNamespace, and the "supporting" schemas have no targetNamespace.
-->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified"
	attributeFormDefault="unqualified">
	
	<xsd:complexType name="Learners">  
    	<xsd:sequence>  
        	<xsd:element name="UserId" type="xsd:string" minOccurs="1" maxOccurs="unbounded" />
   		</xsd:sequence>  
	</xsd:complexType>
	
</xsd:schema>